<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<OutputType>Library</OutputType>
	<TargetFramework>net6.0</TargetFramework>
	<LangVersion>preview</LangVersion>
	<StartupObject></StartupObject>
	<Nullable>enable</Nullable>
	<Platforms>x64;x86</Platforms>
	<SubsystemVersion>6.02</SubsystemVersion>
	<ApplicationIcon />
	<Version>0.0.2</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<ProduceReferenceAssembly>False</ProduceReferenceAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
	<PlatformTarget>x64</PlatformTarget>
	<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
	<PlatformTarget>x64</PlatformTarget>
	<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
	<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x86'">
	<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="AI\Chara\BaseGameIds\**" />
    <EmbeddedResource Remove="AI\Chara\BaseGameIds\**" />
    <None Remove="AI\Chara\BaseGameIds\**" />
  </ItemGroup>

  <ItemGroup>
	<None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
	<PackageReference Include="MessagePack" Version="2.3.75" />
	<PackageReference Include="NLog" Version="5.0.0-preview.1" />
	<PackageReference Include="ObjectDumper.NET" Version="3.2.1-pre" />
  </ItemGroup>

</Project>
